#include<iostream>
using namespace std;
int main(){
    int mat[4][3]={1,2,3,4,5,6,7,89,9,8,7,5};
    int tran[][];
    for(int j=0;j<3;j++){
        for(int i=0;i<4;i++){
            tran[j][i]=mat[i][j];
        }
    }
     for(int i=0;i<3;i++){
        for(int j=0;j<4;j++){
            cout<<tran[i][j];

        }
     }
    
}